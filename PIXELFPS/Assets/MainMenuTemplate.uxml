    </ui:VisualElement>
    <ui:VisualElement name="MultiMenu" class="MenuPanel" style="justify-content: flex-start; align-items: flex-start; padding-left: 50px; padding-right: 50px; padding-top: 50px; -unity-text-align: upper-left; transition-duration: 0.25s; display: flex; background-color: rgba(0, 0, 0, 0.78); visibility: visible;">
